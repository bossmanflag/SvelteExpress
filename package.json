{
	"name": "svelte-express-template",
	"version": "1.0.0",
	"description": "A Simple Template to Develop Svelte + Express Apps!",
	"author": "Manas Hejmadi",
	"license": "ISC",
	"dependencies": {
		"body-parser": "^1.19.0",
		"concurrently": "^5.3.0",
		"cors": "^2.8.5",
		"express": "^4.17.1"
	},
	"scripts": {
		"client-install": "cd client && npm install",
		"serve:server": "nodemon server/index.js",
		"serve:client": "cd client && npm run dev",
		"app": "concurrently \"npm run serve:server\" \"npm run serve:client\""
	},
	"devDependencies": {
		"nodemon": "^2.0.6"
	},
	"main": "server/server.js"
}
